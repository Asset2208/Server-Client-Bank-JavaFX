      <Button fx:id="byAccNumberBtn" layoutX="205.0" layoutY="312.0" mnemonicParsing="false" opacity="0.91" prefHeight="51.0" prefWidth="258.0" style="-fx-background-color: #0B0B3B; -fx-background-radius: 35; -fx-text-fill: white;" text="By account number">
