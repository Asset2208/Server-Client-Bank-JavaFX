        <Button fx:id="backBtn" layoutX="19.0" layoutY="385.0" mnemonicParsing="false" opacity="0.91" prefHeight="38.0" prefWidth="96.0" style="-fx-background-color: #0B0B3B; -fx-background-radius: 100; -fx-text-fill: white;" text="BACK">
